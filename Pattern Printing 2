#include<stdio.h>
int main()
{
int n,i,j;
printf("How many rows do you want? : ");
scanf("%d", &n);

for (i=0 ; i<=n ; i++)
{
	for (j=1 ; j<=i ; j++)
	{
		printf("%d", i);
	}
	printf("\n");
}

return 0;
}

/*
OUTPUT:

How many rows do you want? : 5

1
22
333
4444
55555

--------------------------------

How many rows do you want? : 3

1
22
333

*/
