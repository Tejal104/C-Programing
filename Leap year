/* Check whether the given year is LEAP or not. */

#include<stdio.h>
int main()
{
  int y;
    printf("Enter the year :");
    scanf("%d",&y);
    if(y%4==0)
    {
    	printf("%d is leap year",y);
    }   
    else 
    	printf("%d is not leap year",y);
return 0;
}

/*
OUTPUT:
Enter the year :2020
2020 is leap year
--------------------------------
Enter the year :2021
2021 is not leap year
*/
