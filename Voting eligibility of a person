/* WAP to check whether a person is eligible for voting or not */

#include<stdio.h>
int main()
{
int age;

printf("Enter the age : ");
scanf("%d", &age);

if(age>=18)
{
	printf("Eligible for Voting");
}

else
{
	printf("Not Eligible for Voting");
}

return 0;
}

/*
OUTPUT:
--------------------------------
Enter the age : 12
Not Eligible for Voting
--------------------------------
Enter the age : 18
Eligible for Voting
--------------------------------
Enter the age : 50
Eligible for Voting
--------------------------------
*/
