/* WAP to check whether Number is positive or negative or ZERO. */

#include<stdio.h>
int main()
{
int num;

printf("Enter the number : ");
scanf("%d", &num);

if(num>=0)
{
	if(num>0)
	{
		printf("Number is Positive");
	}
	else
	{
		printf("Number is Zero");
	}
}
else 
	{
    	printf("Number is Negative");
	}
		
return 0;
}

/*
OUTPUT:

Enter the number : 3
Number is Positive
--------------------------------
Enter the number : -3
Number is Negative

*/
