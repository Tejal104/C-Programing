/*  WAP to find the factorial of a number */

#include<stdio.h>
int main()
{
    int n ,i ;
    long int f;
    
    printf("Enter the number : ");
    scanf("%d", &n);
    
    f = 1;
    
    for(i=n ; i>=1 ; i--)
    
    f=f*i;
    
    printf("The factorial of %d is %ld", n, f);    
        
return 0;
}

/*
OUTPUT:
Enter the number : 5
The factorial of 5 is 120
--------------------------------
Enter the number : 10
The factorial of 10 is 3628800
*/
